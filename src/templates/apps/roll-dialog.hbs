<form autocomplete="off">

	{{! <fieldset> }}
	<div style="margin-bottom: 10px">
		<div class="form-group">
			<label>Modificador</label>
			<div class="form-fields">
				<input type="number" name="modificador" placeholder="0" step="1" autofocus />
			</div>
		</div>
		<div class="form-group">
			<label>Maestria</label>
			<div class="form-fields">
				<input type="checkbox" name="maestria" />
			</div>
			<div class="form-fields">
				<input type="number" name="maestriaMod" value="6" max="6" min="1" />
			</div>
		</div>
	</div>
	{{! </fieldset> }}
</form>