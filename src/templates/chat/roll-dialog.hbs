<form autocomplete="off">

	{{! <fieldset> }}
	{{#if isInitiative}}
		<header class="flexrow">
			<img src="{{img}}" width="50" height="50" style="flex: 0 0 50px; margin-right: 5px;" />
			<h3 style="border-bottom: none">{{name}}</h3>
		</header>
	{{/if}}
	<div style="margin-bottom: 10px">
		<div class="form-group">
			<label>Modificador</label>
			<div class="form-fields">
				<input type="number" name="modificador" placeholder="0" step="1" value="{{bonus}}" />
			</div>
		</div>
		<div class="form-group">
			<label>Crítico</label>
			<div class="form-fields">
				<input type="number" name="maestria" value="{{maestria}}" max="6" min="1" />
			</div>
		</div>
		<div class="form-group">
			<label>Sem Críticos</label>
			<div class="form-fields">
				<input type="checkbox" name="semCrit" max="6" min="1" {{checked semCrit}} />
			</div>
		</div>
	</div>
	{{! </fieldset> }}
</form>